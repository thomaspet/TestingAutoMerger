                "DefaultHidden": true
            },
            {
                "Header": "Til",
                "Field": "To"
            },
            {
                "Header": "Melding",
                "Field": "ExternalMessage"
            },
            {
                "Header": "Ekstern refereanse",
                "Field": "ExternalReference",
                "DefaultHidden": true
            },
            {
                "Header": "Forl√∏p",
                "Field": "StatusCode",
                "Alias": "ShareStatus",
                "Format": "SharingStatus"
            }
        ],
        "ParentFilter": {
            "Field": "EntityID",
            "Operator": "eq",
            "Value": "ID"
        }
    }
