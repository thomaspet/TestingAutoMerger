            "DisplayInContextMenu": true,
            "DisplayInActionBar": false,
            "NeedsActionOverride": true
        }, {
            "Name": "Slett",
            "Code": "invoice_delete",
            "Type": "action",
            "ExecuteWithMultipleSelections": false,
            "DisplayInContextMenu": true,
            "DisplayInActionBar": false,
            "NeedsActionOverride": true
