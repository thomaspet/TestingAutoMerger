                    }]
                }]
            },
            {
                "Name": "Kladd",
                "Code": "my_draft_invoices",
                "UseAllCriterias": true,
                "FilterGroups": [{
                    "UseAllCriterias": true,
                    "FieldFilters": [
                    {
                        "Field": "StatusCode",
                        "Operator": "eq",
                        "Value": "42001"
                    }]
                }]
            },
            {
                "Name": "Alle",
                "Code": "all_invoices",
                "UseAllCriterias": true,
                "FilterGroups": [{
                    "UseAllCriterias": false,
                    "FieldFilters": [
                    {
                        "Field": "StatusCode",
                        "Operator": "ne",
                        "Value": "42001"
                    }
                ]}
                ]
            },
            {
                "Name": "Fakturert",
                "Code": "invoiced_invoices",
                "UseAllCriterias": true,
                "FilterGroups": [{
                    "UseAllCriterias": true,
                    "FieldFilters": [
                    {
                        "Field": "StatusCode",
                        "Operator": "gt",
                        "Value": "42001"
                    },
                    {
                        "Field": "StatusCode",
                        "Operator": "lt",
                        "Value": "42004"
                    },
                    {
                        "Field": "InvoiceType",
                        "Operator": "ne",
                        "Value": "1"
                    },
                    {
                        "Field": "IsFullyCredited",
                        "Operator": "ne",
                        "Value": "True"
                    }
                ]}
                ]
            },
            {
                "Name": "Forfalt",
                "Code": "overdue_invoices",
                "OrderBy": "PaymentDueDate ASC",
                "UseAllCriterias": true,
                "FilterGroups": [{
                    "UseAllCriterias": true,
                    "FieldFilters": [
                    {
                        "Field": "RestAmountCurrency",
                        "Operator": "ne",
                        "Value": "0"
                    },
                    {
                        "Field": "PaymentDueDate",
                        "Operator": "lt",
                        "Value": "getdate()"
                    },
                    {
                        "Field": "IsFullyCredited",
                        "Operator": "ne",
                        "Value": "True"
                    }
                ]},
                {
                    "UseAllCriterias": false,
                    "FieldFilters": [
                    {
                        "Field": "StatusCode",
                        "Operator": "eq",
                        "Value": "42002"
                    },
                    {
                        "Field": "StatusCode",
                        "Operator": "eq",
                        "Value": "42003"
                    }
                ]}
                ]
            },
            {
                "Name": "Purret",
                "Code": "invoices_reminded",
                "UseAllCriterias": true,
                "FilterGroups": [{
                    "UseAllCriterias": true,
                    "FieldFilters": [
                    {
                        "Field": "CollectorStatusCode",
                        "Operator": "eq",
                        "Value": "42501"
                    },
                    {
                        "Field": "IsFullyCredited",
                        "Operator": "ne",
                        "Value": "True"
                    }
                ]}
                ]
            },
            {
                "Name": "Sendt til inkasso",
                "Code": "invoices_sentforcollection",
                "UseAllCriterias": true,
                "FilterGroups": [{
                    "UseAllCriterias": true,
                    "FieldFilters": [
                    {
                        "Field": "CollectorStatusCode",
                        "Operator": "eq",
                        "Value": "42502"
                    }
                ]}
                ]
            },
            {
                "Name": "Betalt",
                "Code": "paid_invoices",
                "UseAllCriterias": true,
                "FilterGroups": [{
                    "UseAllCriterias": true,
                    "FieldFilters": [
                    {
                        "Field": "StatusCode",
                        "Operator": "eq",
                        "Value": "42004"
                    },
                    {
                        "Field": "IsFullyCredited",
                        "Operator": "ne",
                        "Value": "True"
                    }
                ]}
                ]
            },
            {
                "Name": "Kreditert",
                "Code": "fullyCredited_invoices",
                "UseAllCriterias": true,
                "FilterGroups": [{
                    "UseAllCriterias": false,
                    "FieldFilters": [
                    {
                        "Field": "IsFullyCredited",
                        "Operator": "eq",
                        "Value": "True"
                    },
                    {
                        "Field": "InvoiceType",
                        "Operator": "eq",
                        "Value": "1"
                    }
                ]}
                ]
            },
            {
                "Name": "Avansert filter",
                "Code": "due_invoices",
                "Filter": "PaymentDueDate lt getdate()"
            }
        ]
