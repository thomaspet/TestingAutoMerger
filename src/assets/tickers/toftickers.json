            "Width": "150px",
            "FilterOperator": "gt"
        }, {
            "Header": "Rest",
            "Field": "RestAmountCurrency",
            "Format": "NumberPositiveNegative",
            "Width": "150px",
            "FilterOperator": "gt"
        }, {
            "Header": "Kreditert",
            "Field": "CreditedAmountCurrency",
            "Format": "NumberPositiveNegative",
