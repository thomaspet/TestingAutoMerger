{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "economy-frontend": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "indexTransform": "./index-html-transform.js",
            "customWebpackConfig": {
              "path": "./webpack.config.js"
            },
            "allowedCommonJsDependencies": [
              "lodash",
              "chart.js",
              "file-saver",
              "chartjs-plugin-datalabels",
              "chartjs-plugin-doughnutlabel",
              "ag-grid-angular",
              "moment/locale/nb"
           ],
            "aot": false,
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/config",
              "src/assets",
              "src/robots.txt",
              "src/security.txt",
              "src/fileviewer.html",
              "src/ehf-print.html",
              "src/our_suppliers.html"
            ],
            "styles": [
              "src/styles.sass",
              "src/assets/bootstrap-grid/bootstrap-grid.min.css",
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/ag-grid-community/dist/styles/ag-grid.css",
              "node_modules/perfect-scrollbar/css/perfect-scrollbar.css",
              "node_modules/@ng-select/ng-select/themes/default.theme.css",
              {
                "input": "src/assets/ehf-viewer.css",
                "bundleName": "ehf-viewer"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/themes/ue"
              ]
            }
          },
          "configurations": {
            "serve-app": {
              "aot": false,
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": false,
              "buildOptimizer": false,
              "outputHashing": "none",
              "sourceMap": false,
              "namedChunks": true,
              "vendorChunk": true
            },
            "devmode": {
              "aot": true,
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "buildOptimizer": false,
              "outputHashing": "all",
              "sourceMap": {
                "scripts": true,
                "styles": false,
                "vendor": false
              },
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false
            },
            "prodmode": {
              "aot": true,
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "buildOptimizer": true,
              "outputHashing": "all",
              "sourceMap": {
                "scripts": true,
                "styles": false,
                "vendor": false
              },
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false
            },
            "aot-error-check": {
              "aot": true,
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": false,
              "buildOptimizer": false,
              "outputHashing": "none",
              "sourceMap": false,
              "namedChunks": false,
              "vendorChunk": true
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "economy-frontend:build"
          },
          "configurations": {
            "serve-app": {
              "browserTarget": "economy-frontend:build:serve-app"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-builders/custom-webpack:extract-i18n",
          "options": {
            "browserTarget": "economy-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-builders/custom-webpack:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
            ],
            "styles": [
              "src/styles.sass",
              "src/assets/bootstrap-grid/bootstrap-grid.min.css",
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/angular-tree-component/dist/angular-tree-component.css",
              "node_modules/ag-grid/dist/styles/ag-grid.css",
              "node_modules/perfect-scrollbar/css/perfect-scrollbar.css",
              "node_modules/@ng-select/ng-select/themes/default.theme.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.svg",
              "src/robots.txt",
              "src/security.txt"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "economy-frontend-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "economy-frontend:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "economy-frontend",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "uni",
      "style": "sass"
    },
    "@schematics/angular:directive": {
      "prefix": "uni"
    }
  },
  "cli": {
    "analytics": "8f54a5c5-a2b8-4ebe-b1f8-13047dddc762"
  }
}