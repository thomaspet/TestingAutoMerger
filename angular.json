{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "economy-frontend": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.png",
              "src/robots.txt",
              "src/security.txt"
            ],
            "styles": [
              "src/styles.sass",
              "src/assets/bootstrap-grid/bootstrap-grid.min.css",
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/angular-tree-component/dist/angular-tree-component.css",
              "node_modules/ag-grid/dist/styles/ag-grid.css",
              "node_modules/perfect-scrollbar/css/perfect-scrollbar.css",
              "node_modules/@ng-select/ng-select/themes/default.theme.css"
            ],
            "scripts": [
              "src/raygun-setup.js"
            ]
          },
          "configurations": {
            "dev": {
              "aot": true,
              "optimization": true,
              "buildOptimizer": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false
            },
            "dev-serve": {
              "aot": false,
              "optimization": false,
              "buildOptimizer": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": false,
              "namedChunks": true,
              "vendorChunk": false
            },
            "test": {
              "aot": true,
              "optimization": true,
              "buildOptimizer": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.test.ts"
                }
              ]
            },
            "test-serve": {
              "aot": false,
              "optimization": false,
              "buildOptimizer": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": false,
              "namedChunks": true,
              "vendorChunk": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.test.ts"
                }
              ]
            },
            "prod": {
              "aot": true,
              "optimization": true,
              "buildOptimizer": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "prod-serve": {
              "aot": false,
              "optimization": false,
              "buildOptimizer": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": false,
              "namedChunks": true,
              "vendorChunk": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "local": {
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.local.ts"
                }
              ]
            },
            "aot-error-check": {
              "aot": true,
              "optimization": false,
              "buildOptimizer": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "vendorChunk": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "economy-frontend:build"
          },
          "configurations": {
            "dev-serve": {
              "browserTarget": "economy-frontend:build:dev-serve"
            },
            "test-serve": {
              "browserTarget": "economy-frontend:build:test-serve"
            },
            "prod-serve": {
              "browserTarget": "economy-frontend:build:prod-serve"
            },
            "local": {
              "browserTarget": "economy-frontend:build:local"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "economy-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "src/raygun-setup.js"
            ],
            "styles": [
              "src/styles.sass",
              "src/assets/bootstrap-grid/bootstrap-grid.min.css",
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/angular-tree-component/dist/angular-tree-component.css",
              "node_modules/ag-grid/dist/styles/ag-grid.css",
              "node_modules/perfect-scrollbar/css/perfect-scrollbar.css",
              "node_modules/@ng-select/ng-select/themes/default.theme.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.png",
              "src/robots.txt",
              "src/security.txt"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "economy-frontend-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "economy-frontend:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "economy-frontend",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "uni",
      "styleext": "sass"
    },
    "@schematics/angular:directive": {
      "prefix": "uni"
    }
  }
}