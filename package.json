    "file-saver": "1.3.2",
    "immutable": "^3.8.1",
    "jquery": "^2.1.4",
    "jwt-decode": "^1.4.0",
    "lodash": "^4.17.2",
    "rxjs": "5.0.1",
    "typings": "^2.1.0",
    "uniform-ng2": "^0.0.48",
    "unitable-ng2": "0.0.5",
    "unisearch-ng2": "0.0.17",
