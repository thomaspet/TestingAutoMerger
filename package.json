{
  "name": "uni-economy-frontend",
  "version": "0.0.0",
  "license": "",
  "private": true,
  "scripts": {
    "set-config": "rimraf src/config/dist && node src/config/set-config.js",
    "config-selector": "rimraf src/config/dist && node src/config/config-selector.js",
    "config-watcher": "node src/config/config-watcher.js",

    "serve-app": "npm run prebuild && npm run high-memory-build -- serve --port=3000 --configuration=serve-app --proxy-config src/config/dist/proxy.json",
    "build-devmode": "npm run prebuild && npm run high-memory-build -- build --configuration=devmode",
    "build-prodmode": "npm run prebuild && npm run high-memory-build -- build --configuration=prodmode",

    "start": "npm-run-all config-selector --parallel config-watcher serve-app",

    "build.dev":          "npm-run-all \"set-config theme=ue env=dev\" build-devmode",
    "build.test":         "npm-run-all \"set-config theme=ue env=test\" build-devmode",
    "build.rc":           "npm-run-all \"set-config theme=ue env=rc\" build-prodmode",
    "build.prod":         "npm-run-all \"set-config theme=ue env=prod\" build-prodmode",
    "build.ext01":        "npm-run-all \"set-config theme=ext01 env=ext01\" build-prodmode",
    "build.ext01-rc":     "npm-run-all \"set-config theme=ext01 env=ext01-rc\" build-prodmode",
    "build.ext02":        "npm-run-all \"set-config theme=ext02 env=ext02\" build-prodmode",
    "build.ext02-rc":     "npm-run-all \"set-config theme=ext02 env=ext02-rc\" build-prodmode",
    "build.softrig":      "npm-run-all \"set-config theme=softrig env=softrig\" build-prodmode",
    "build.softrig-dev":  "npm-run-all \"set-config theme=softrig env=dev\" build-devmode",
    "build.ext03-poc01":  "npm-run-all \"set-config theme=ue env=ext03-poc01\" build-devmode",

    "postinstall": "node ./scripts/post-install.js",
    "license-check": "node ./scripts/license-check.js",
    "video-mapping": "node ./scripts/video-mapping.js",
    "entities": "node ./scripts/entities.js",
    "prebuild": "node ./scripts/prebuild.js",
    "zip-dist": "node ./scripts/zip.js",
    "check-aot": "npm run high-memory-build -- build --configuration=aot-error-check",
    "high-memory-build": "node --max_old_space_size=6144 node_modules/@angular/cli/bin/ng"
  },
  "dependencies": {
    "@angular-builders/custom-webpack": "^11.0.0",
    "@angular/animations": "~11.0.5",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@aspnet/signalr": "^1.1.4",
    "@microsoft/applicationinsights-web": "^2.5.5",
    "@ng-select/ng-select": "^5.0.15",
    "ag-grid-angular": "^19.0.0",
    "ag-grid-community": "19.0.0",
    "ag-grid-enterprise": "19.0.0",
    "applicationinsights-js": "1.0.20",
    "chart.js": "^2.9.3",
    "chartjs-plugin-datalabels": "0.6.0",
    "chartjs-plugin-doughnutlabel": "2.0.3",
    "file-saver": "^2.0.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.20",
    "marked": "^1.2.7",
    "moment": "^2.19.2",
    "muuri": "^0.8.0",
    "oidc-client": "^1.10.1",
    "perfect-scrollbar": "^1.5.0",
    "print-js": "^1.0.63",
    "rxjs": "6.5.4",
    "rxjs-compat": "6.3.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.5",
    "@angular/cli": "^11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@angular/language-service": "~11.0.5",
    "@types/applicationinsights-js": "1.0.9",
    "@types/chart.js": "^2.7.55",
    "@types/lodash": "^4.14.118",
    "@types/marked": "^1.2.1",
    "@types/node": "^13.7.0",
    "archiver": "^3.0.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "license-checker": "^15.0.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "protractor": "~7.0.0",
    "enquirer": "^2.3.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sync-request": "6.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
}
