{
  "name": "uni-economy-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "license-check": "node ./scripts/license-check.js",
    "video-mapping": "node ./scripts/video-mapping.js",
    "entities": "node ./scripts/entities.js",
    "prebuild": "node ./scripts/prebuild.js",
    "zip-dist": "node ./scripts/zip.js",
    "build.dev": "npm run prebuild && ng build --configuration=dev --output-hashing=all --no-aot",
    "build.test": "npm run prebuild && ng build --configuration=test --output-hashing=all --no-aot",
    "build.test.prodmode": "npm run prebuild && ng build --configuration=test --prod",
    "build.pilot": "npm run prebuild && ng build --configuration=pilot --prod",
    "build.prod": "npm run prebuild && ng build --configuration=prod --prod --no-aot --build-optimizer=false",
    "start": "npm run start.dev",
    "start.dev": "npm run prebuild && ng serve --port=3000 --configuration=dev --proxy-config proxy/dev.proxy.json",
    "start.test": "npm run prebuild && ng serve --port=3000 --configuration=test --proxy-config proxy/test.proxy.json",
    "start.local": "npm run prebuild && ng serve --port=3000 --configuration=local --proxy-config proxy/local.proxy.json",
    "start.pilot": "npm run prebuild && ng serve --port=3000 --configuration=pilot --proxy-config proxy/pilot.proxy.json",

    "start.cheap": "npm run prebuild && ng serve --port=3000 --configuration=dev --eval-source-map=true --proxy-config proxy/dev.proxy.json",
    "start.local.cheap": "npm run prebuild && ng serve --port=3000 --configuration=local --eval-source-map=true --proxy-config proxy/local.proxy.json",

    "check-aot": "ng build --configuration=aot-error-check",

    "high-memory-build": "node --max_old_space_size=6144 node_modules/@angular/cli/bin/ng",
    "build.dev.aot": "npm run prebuild && npm run high-memory-build -- build --configuration=dev-aot"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/cdk": "^6.0.3",
    "@angular/material": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@ng-select/ng-select": "^1.3.0",
    "ag-grid": "^16.0.1",
    "ag-grid-angular": "^16.0.0",
    "angular-tree-component": "^7.0.1",
    "chart.js": "^2.1.6",
    "core-js": "^2.4.1",
    "file-saver": "^1.3.8",
    "immutable": "^3.8.2",
    "jquery": "^3.2.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "ng-recaptcha": "^3.0.2",
    "perfect-scrollbar": "^1.3.0",
    "raygun4js": "^2.7.1",
    "rxjs": "^6.2.0",
    "zone.js": "^0.8.26",
    "rxjs-compat": "^6.2.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.6",
    "@angular/cli": "^6.0.7",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "archiver": "^2.1.0",
    "codelyzer": "^4.3.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "license-checker": "^15.0.0",
    "protractor": "~5.1.2",
    "sync-request": "^4.1.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "2.7.2"
  }
}