{
  "name": "uni-economy-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "license-check": "node ./scripts/license-check.js",
    "video-mapping": "node ./scripts/video-mapping.js",
    "entities": "node ./scripts/entities.js",
    "prebuild": "node ./scripts/prebuild.js",
    "zip-dist": "node ./scripts/zip.js",
    "build.dev": "npm run prebuild && ng build --env=dev --output-hashing=all --no-aot",
    "build.test": "npm run prebuild && ng build --env=test --output-hashing=all --no-aot",
    "build.test.prodmode": "npm run prebuild && ng build --env=test --prod --no-aot",
    "build.pilot": "npm run prebuild && ng build --env=pilot --prod --no-aot",
    "build.prod": "npm run prebuild && ng build --env=prod --prod --no-aot",
    "start": "npm run start.dev",
    "start.dev": "npm run prebuild && ng serve --port=3000 --env=dev --proxy-config proxy/dev.proxy.json",
    "start.test": "npm run prebuild && ng serve --port=3000 --env=test --proxy-config proxy/test.proxy.json",
    "start.local": "npm run prebuild && ng serve --port=3000 --env=local --proxy-config proxy/local.proxy.json",
    "start.pilot": "npm run prebuild && ng serve --port=3000 --env=pilot --proxy-config proxy/pilot.proxy.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "angular-tree-component": "^5.2.1",
    "chart.js": "^2.1.6",
    "core-js": "^2.4.1",
    "file-saver": "1.3.2",
    "immutable": "^3.8.2",
    "jquery": "^3.2.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "raygun4js": "^2.7.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.0-rc.1",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "archiver": "^2.1.0",
    "codelyzer": "~3.2.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "license-checker": "^15.0.0",
    "protractor": "~5.1.2",
    "sync-request": "^4.1.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
