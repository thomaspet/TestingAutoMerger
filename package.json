{
  "name": "angular2-unieconomy-frontend",
  "version": "0.0.1",
  "scripts": {
    "sass.watch": "gulp sass.watch",
    "build.vendors": "webpack --config config/vendors.webpack.js",
    "postinstall": "webpack --config config/vendors.webpack.js",
    "entities": "gulp entities",
    "build.dev": "gulp sass && gulp app-config-dev && webpack --config config/dev.webpack.js",
    "build.test": "gulp sass && gulp app-config-test-env && webpack --config config/dev.webpack.js",
    "build.pilot": "gulp sass && gulp app-config-pilot && webpack --config config/prod.webpack.js",
    "build.prod": "gulp sass && gulp app-config-prod && webpack --config config/prod.webpack.js",
    "start": "npm run start.dev",
    "start.dev": "gulp app-config-dev && gulp sass && concurrently \"npm run sass.watch\" \"webpack-dev-server --inline --port 3000\" ",
    "start.local": "gulp app-config-local && gulp sass &&  concurrently \"npm run sass.watch\" \"webpack-dev-server --inline --port 3000\" ",
    "start.pilot": "gulp app-config-pilot && gulp sass &&  concurrently \"npm run sass.watch\" \"webpack-dev-server --config config/prod.webpack.js --inline --port 3000\" "
  },
  "license": "MIT",
